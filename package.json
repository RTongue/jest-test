{
  "name": "jest-test",
  "version": "1.0.0",
  "description": "Minimal jest typescript setup",
  "author": "RTongue @RTongue",
  "bin": {
    "lunch-money": "./bin/run"
  },
  "homepage": "https://github.com/RTongue/jest-test",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "RTongue/jest-test",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.7.0"
  },
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.4",
    "@swc/core": "^1.3.86",
    "@swc/jest": "^0.2.29",
    "@types/chai": "^4",
    "@types/jest": "^29.5.5",
    "@types/node": "^16.18.51",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "jest": "^29.7.0",
    "oclif": "^3.16.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "lunch-money",
    "dirname": "lunch-money",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      },
      "clear": {
        "description": "Categorize and clear your most recent transactions"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "jest --config jest.config.ts",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/RTongue/lunch-money/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
